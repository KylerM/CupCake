<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
    <metadata>
      <id>CupCake</id>
      <version>$version$</version>
      <title>CupCake</title>
      <authors>Yonom</authors>
      <owners>Yonom</owners>
      <iconUrl>http://cupcake.yonom.org/img/CupCake.png</iconUrl>
      <requireLicenseAcceptance>false</requireLicenseAcceptance>
      <description>CupCake can be used to write plugins for CupCake.Server</description>
    </metadata>
    <files>
      <file src="bin\Release\Cupcake.*.dll" target="lib\net40" />
      <file src="bin\Release\MuffinFramework.dll" target="lib\net40" />
      <file src="..\CupCake.Debug\bin\Release\CupCake.Debug.exe" target="content" />
      <file src="tools\install.ps1" target="tools" />
      <file src="tools\uninstall.ps1" target="tools" />
      <file src="build\CupCake.props" target="build" />
    </files>
</package>